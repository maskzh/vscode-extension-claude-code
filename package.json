{
  "name": "claude-code-integration",
  "displayName": "%extension.displayName%",
  "description": "%extension.description%",
  "icon": "icons/logo.png",
  "version": "0.0.6",
  "publisher": "maskzh",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maskzh/vscode-extension-claude-code.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "zh-cn",
        "path": "./package.nls.zh-cn.json"
      }
    ],
    "commands": [
      {
        "command": "claudeCodeIntegration.qwen",
        "title": "%command.qwen.title%",
        "icon": {
          "light": "./icons/qwen.svg",
          "dark": "./icons/qwen-dark.svg"
        }
      },
      {
        "command": "claudeCodeIntegration.kimi",
        "title": "%command.kimi.title%",
        "icon": {
          "light": "./icons/kimi.svg",
          "dark": "./icons/kimi-dark.svg"
        }
      },
      {
        "command": "claudeCodeIntegration.deepseek",
        "title": "%command.deepseek.title%",
        "icon": {
          "light": "./icons/deepseek.svg",
          "dark": "./icons/deepseek-dark.svg"
        }
      },
      {
        "command": "claudeCodeIntegration.minimax",
        "title": "%command.minimax.title%",
        "icon": {
          "light": "./icons/minimax.svg",
          "dark": "./icons/minimax-dark.svg"
        }
      },
      {
        "command": "claudeCodeIntegration.zhipu",
        "title": "%command.zhipu.title%",
        "icon": {
          "light": "./icons/zhipu.svg",
          "dark": "./icons/zhipu-dark.svg"
        }
      },
      {
        "command": "claudeCodeIntegration.copilot",
        "title": "%command.copilot.title%",
        "icon": {
          "light": "./icons/copilot.svg",
          "dark": "./icons/copilot-dark.svg"
        }
      },
      {
        "command": "claudeCodeIntegration.doubao",
        "title": "%command.doubao.title%",
        "icon": {
          "light": "./icons/doubao.svg",
          "dark": "./icons/doubao-dark.svg"
        }
      },
      {
        "command": "claudeCodeIntegration.custom",
        "title": "%command.custom.title%",
        "icon": "$(terminal)"
      },
      {
        "command": "claudeCodeIntegration.configure",
        "title": "%command.configure.title%",
        "icon": "$(gear)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "claudeCodeIntegration.qwen",
          "when": "claudeExtension.qwen.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.kimi",
          "when": "claudeExtension.kimi.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.deepseek",
          "when": "claudeExtension.deepseek.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.minimax",
          "when": "claudeExtension.minimax.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.zhipu",
          "when": "claudeExtension.zhipu.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.copilot",
          "when": "claudeExtension.copilot.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.doubao",
          "when": "claudeExtension.doubao.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.custom",
          "when": "claudeExtension.custom.visible",
          "group": "navigation@-9"
        },
        {
          "command": "claudeCodeIntegration.configure",
          "group": "1_run"
        }
      ]
    },
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "claudeCodeIntegration.qwen.command": {
          "type": "string",
          "description": "%configuration.qwen.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.qwen.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "https://dashscope.aliyuncs.com/api/v2/apps/claude-code-proxy",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "qwen3-coder-plus",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "qwen3-coder-plus",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "qwen3-coder-plus",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.qwen.env.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.kimi.command": {
          "type": "string",
          "description": "%configuration.kimi.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.kimi.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "https://api.moonshot.cn/anthropic",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "kimi-k2-thinking-turbo",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "kimi-k2-turbo-preview",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "kimi-k2-turbo-preview",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.kimi.env.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.deepseek.command": {
          "type": "string",
          "description": "%configuration.deepseek.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.deepseek.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "https://api.deepseek.com/anthropic",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "deepseek-reasoner",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "deepseek-chat",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "deepseek-chat",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.deepseek.env.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.minimax.command": {
          "type": "string",
          "description": "%configuration.minimax.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.minimax.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "https://api.minimax.io/anthropic",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "MiniMax-M2",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "MiniMax-M2",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "MiniMax-M2",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.minimax.env.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.zhipu.command": {
          "type": "string",
          "description": "%configuration.zhipu.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.zhipu.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "https://open.bigmodel.cn/api/anthropic",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "glm-4.6",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "glm-4.6",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "glm-4.6-air",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.zhipu.env.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.copilot.command": {
          "type": "string",
          "description": "%configuration.copilot.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.copilot.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "claude-opus-4.5",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "claude-sonnet-4.5",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "claude-haiku-4.5",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.copilot.env.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.doubao.command": {
          "type": "string",
          "description": "%configuration.doubao.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.doubao.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "https://ark.cn-beijing.volces.com/api/compatible",
            "ANTHROPIC_MODEL": "doubao-seed-code-preview-latest",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "doubao-seed-code-preview-latest",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "doubao-seed-code-preview-latest",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "doubao-seed-code-preview-latest",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.doubao.env.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.custom.command": {
          "type": "string",
          "description": "%configuration.custom.command.description%",
          "scope": "window"
        },
        "claudeCodeIntegration.custom.env": {
          "type": "object",
          "default": {
            "ANTHROPIC_BASE_URL": "",
            "ANTHROPIC_DEFAULT_OPUS_MODEL": "claude-opus-4.5",
            "ANTHROPIC_DEFAULT_SONNET_MODEL": "claude-sonnet-4.5",
            "ANTHROPIC_DEFAULT_HAIKU_MODEL": "claude-haiku-4.5",
            "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": 1
          },
          "markdownDescription": "%configuration.custom.env.description%",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts && npx commitlint --from=HEAD~1 --to=HEAD --verbose",
    "package": "vsce package",
    "release": "standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry": "standard-version --dry-run",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/node": "24.1.0",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "standard-version": "^9.5.0",
    "typescript": "^5.8.3"
  }
}
