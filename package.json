{
  "name": "claude-code-terminal",
  "displayName": "%extension.displayName%",
  "description": "%extension.description%",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maskzh/vscode-extension-claude-code.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "zh-cn",
        "path": "./package.nls.zh-cn.json"
      }
    ],
    "commands": [
      {
        "command": "claude-code-terminal.qwen",
        "title": "%command.qwen.title%",
        "icon": {
          "light": "./icons/qwen.svg",
          "dark": "./icons/qwen-dark.svg"
        }
      },
      {
        "command": "claude-code-terminal.kimi",
        "title": "%command.kimi.title%",
        "icon": {
          "light": "./icons/kimi.svg",
          "dark": "./icons/kimi-dark.svg"
        }
      },
      {
        "command": "claude-code-terminal.deepseek",
        "title": "%command.deepseek.title%",
        "icon": {
          "light": "./icons/deepseek.svg",
          "dark": "./icons/deepseek-dark.svg"
        }
      },
      {
        "command": "claude-code-terminal.copilot",
        "title": "%command.copilot.title%",
        "icon": {
          "light": "./icons/copilot.svg",
          "dark": "./icons/copilot-dark.svg"
        }
      },
      {
        "command": "claude-code-terminal.custom",
        "title": "%command.custom.title%",
        "icon": "$(terminal)"
      },
      {
        "command": "claude-code-terminal.configure",
        "title": "%command.configure.title%",
        "icon": "$(gear)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "claude-code-terminal.qwen",
          "when": "resourceExtname != '' && claudeExtension.qwen.visible",
          "group": "navigation"
        },
        {
          "command": "claude-code-terminal.kimi",
          "when": "resourceExtname != '' && claudeExtension.kimi.visible",
          "group": "navigation"
        },
        {
          "command": "claude-code-terminal.deepseek",
          "when": "resourceExtname != '' && claudeExtension.deepseek.visible",
          "group": "navigation"
        },
        {
          "command": "claude-code-terminal.copilot",
          "when": "resourceExtname != '' && claudeExtension.copilot.visible",
          "group": "navigation"
        },
        {
          "command": "claude-code-terminal.custom",
          "when": "resourceExtname != '' && claudeExtension.custom.visible",
          "group": "navigation"
        },
        {
          "command": "claude-code-terminal.configure",
          "when": "resourceExtname != ''",
          "group": "1_run"
        }
      ]
    },
    "configuration": {
      "title": "%configuration.title%",
      "properties": {
        "ClaudeCodeTerminal.qwen.baseUrl": {
          "type": "string",
          "default": "https://dashscope.aliyuncs.com/api/v2/apps/claude-code-proxy",
          "description": "%configuration.qwen.baseUrl.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.kimi.baseUrl": {
          "type": "string",
          "default": "https://api.moonshot.cn/anthropic",
          "description": "%configuration.kimi.baseUrl.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.custom.baseUrl": {
          "type": "string",
          "default": "",
          "description": "%configuration.custom.baseUrl.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.qwen.command": {
          "type": "string",
          "description": "%configuration.qwen.command.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.kimi.command": {
          "type": "string",
          "description": "%configuration.kimi.command.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.deepseek.baseUrl": {
          "type": "string",
          "default": "https://api.deepseek.com/anthropic",
          "description": "%configuration.deepseek.baseUrl.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.deepseek.command": {
          "type": "string",
          "description": "%configuration.deepseek.command.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.copilot.baseUrl": {
          "type": "string",
          "default": "https://api.github.com/copilot/anthropic",
          "description": "%configuration.copilot.baseUrl.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.copilot.command": {
          "type": "string",
          "description": "%configuration.copilot.command.description%",
          "scope": "window"
        },
        "ClaudeCodeTerminal.custom.command": {
          "type": "string",
          "description": "%configuration.custom.command.description%",
          "scope": "window"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/node": "24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "typescript": "^5.8.3"
  }
}
